{
    "type": "partition",
    "state": "enabled",
    "development_mode_enabled": false,
    "service_name": "mongodb-atlas",
    "client_max_offline_days": 30,
    "is_recovery_mode_disabled": false,
    "partition": {
        "key": "name",
        "type": "string",
        "permissions": {
            "read": {
                "%%partition": "%%user.id"
